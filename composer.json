{
    "name": "stubbles/values",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Useful classes to ease the work with values.",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "mikey179/vfsStream": "^1.6",
        "bovigo/assert": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\values\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
