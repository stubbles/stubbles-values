{
    "name": "stubbles/values",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Useful classes to ease the work with values.",
    "require": {
        "php": "^8.3",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "mikey179/vfsstream": "^1.6.12",
        "bovigo/callmap": "^9.0",
        "bovigo/assert": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\values\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\streams\\file\\": "src/test/helper",
            "stubbles\\values\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "12.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
