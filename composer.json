{
    "name": "stubbles/values",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Useful classes to ease the work with values.",
    "require": {
        "php": "^7.0",
        "ext-iconv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mikey179/vfsStream": "^1.6",
        "bovigo/callmap": "^3.0",
        "bovigo/assert": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\values\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\streams\\file\\": "src/test/helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
